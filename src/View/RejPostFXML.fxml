<AnchorPane id="AnchorPane" prefHeight="447.0" prefWidth="627.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.RejPostFXMLController">
