<AnchorPane id="AnchorPane" prefHeight="441.0" prefWidth="788.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ClientFXMLController">
